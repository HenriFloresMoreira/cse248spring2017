package p2_GUI;

public class Student {
	private String name;
	private String idNum;

	public Student(String name, String idNum) {
		super();
		this.name = name;
		this.idNum = idNum;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIdNum() {
		return idNum;
	}

	public void setIdNum(String idNum) {
		this.idNum = idNum;
	}

}
