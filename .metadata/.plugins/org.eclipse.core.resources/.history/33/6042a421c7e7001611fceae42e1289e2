package comparator;

import java.util.Comparator;

public class CountryComparator implements Comparator<Country>{

	@Override
	public int compare(Country o1, Country o2) {
		if(o1.getArea() - o2.getArea() > 0) {
			return 1;
		}
		if(o1.getArea() - o2.getArea() < 0) {
			return -1;
		}
		return 0;
		
//		return Double.compare(o1.getArea(), o2.getArea());
	}

}
