package dependency;

public class Demo {

	public static void main(String[] args) {
		Course c1 = new Course("CSE118", "Intro to Java", 3, "A");
		Course c2 = new Course("CSE148", "Object Oriented Programming", 4, "A");
		Courses courses = new Courses();
		courses.add(c1);
		courses.add(c2);
		System.out.println(courses.remove("CSE168"));
		
		System.out.println(courses.getGPA());
		System.out.println(courses.toString());
	}

}
