package inheritance_1;

public class Demo {

	public static void main(String[] args) {
		MallardDuck duck1 = new MallardDuck();
		duck1.swim();
		duck1.quack();
		duck1.fly();
		duck1.display();
		
		RedheadDuck duck2 = new RedheadDuck();
		duck2.swim();
		duck2.quack();
		duck2.fly();
		duck2.display();
	}

}
